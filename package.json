  "repository": {
    "type": "git",
    "url": "https://github.com/jembezmamy/morpher-js.git"
  },
  "bugs": {
    "url": "https://github.com/jembezmamy/morpher-js/issues"
  },
  "homepage": "http://jembezmamy.github.io/morpher-js/",
  "devDependencies": {
    "@vitejs/plugin-react": "^5.0.4",
    "terser": "^5.44.0",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
