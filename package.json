{
  "name": "morpher-js",
  "version": "2.0.0",
  "description": "JavaScript image morphing library using HTML5 Canvas",
  "type": "module",
  "main": "./dist/morpher.cjs",
  "module": "./dist/morpher.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/morpher.js",
      "require": "./dist/morpher.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "echo \"No tests yet\" && exit 0",
    "lint": "echo \"No linter configured yet\" && exit 0",
    "old:dev": "echo \"Use 'bundle exec middleman server' to run old Middleman server\" && exit 1",
    "old:build": "echo \"Use 'bundle exec middleman build' to run old Middleman build\" && exit 1"
  },
  "keywords": [
    "morphing",
    "canvas",
    "image",
    "animation",
    "blend",
    "mesh",
    "triangle"
  ],
  "author": "PaweÅ‚ Bator",
